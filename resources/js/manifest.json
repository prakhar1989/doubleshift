{
  "name": "doubleshift",
  "version": "0.0.1",
  "browser_action": {
      "default_title": "Show the popup",
      "default_popup": "popup.html",
      "default_icon": {
        "19": "icon19.png",
        "38": "icon38.png"
      }

  },
  "permissions": ["tabs"],
  "commands":{
    "_execute_browser_action": {
      "suggested_key": {
	"default": "Ctrl+Shift+Space"
      },
      "description": "Show the Double Shift Pop-up"
    }
  },
  "background": {
      "page": "bg.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}

